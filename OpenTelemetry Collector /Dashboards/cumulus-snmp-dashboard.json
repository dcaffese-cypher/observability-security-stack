  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "type": "table",
      "title": "Down interfaces (all Cumulus switches)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "ifOperStatus == 2",
          "legendFormat": "{{instance}} {{ifName}}",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": false,
            "displayName": "instance"
          }
        ]
      },
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {
            "mode": "columns"
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Value": false
            },
            "indexByName": {
              "Time": 0,
              "instance": 1,
              "ifName": 2,
              "ifDescr": 3,
              "ifAlias": 4,
              "Value": 5
            },
            "renameByName": {
              "Value": "status (1=UP,2=DOWN)"
            }
          }
        }
      ]
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Down ports per switch",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 10
      },
      "targets": [
        {
          "refId": "A",
          "expr": "count by (instance) (ifOperStatus == 2)",
          "legendFormat": "{{instance}}",
          "range": true
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          },
          "unit": "none"
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Interface IN traffic (bps)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": {
        "h": 8,
        "w": 16,
        "x": 8,
        "y": 10
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(ifHCInOctets{instance=\"$cumulus_switch\",ifName=\"$interface\"}[5m]) * 8",
          "legendFormat": "IN {{instance}} {{ifName}}",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bps",
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Interface OUT traffic (bps)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": {
        "h": 8,
        "w": 16,
        "x": 8,
        "y": 18
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(ifHCOutOctets{instance=\"$cumulus_switch\",ifName=\"$interface\"}[5m]) * 8",
          "legendFormat": "OUT {{instance}} {{ifName}}",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bps",
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    }
  ],
