      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 22 },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(ifHCInOctets{instance=\"$cumulus_switch\",ifName=\"$interface\"}[$__rate_interval]) * 8",
          "legendFormat": "IN {{instance}} {{ifName}}",
          "range": true
