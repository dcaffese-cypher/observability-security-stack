      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [ { "color": "green", "value": null } ]
          },
          "unit": "none"
        },
        "overrides": []
      }
    },
    {
      "id": 6,
