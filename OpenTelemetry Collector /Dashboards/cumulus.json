{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
          "datasource": {
            "type": "grafana",
            "uid": "-- Grafana --"
          },
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "type": "table",
      "title": "All interfaces (OperStatus) – selected switch",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "ifOperStatus{instance=\"$cumulus_switch\"}",
          "legendFormat": "{{instance}} {{ifName}}",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto", "displayMode": "auto" }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [ { "desc": false, "displayName": "ifName" } ]
      },
      "transformations": [
        { "id": "labelsToFields", "options": { "mode": "columns" } },
        {
          "id": "organize",
          "options": {
            "excludeByName": {},
            "indexByName": {
              "Time": 0,
              "ifName": 1,
              "ifDescr": 2,
              "ifAlias": 3,
              "Value": 4,
              "ifIndex": 5,
              "instance": 6,
              "job": 7,
              "__name__": 8
            },
            "renameByName": {
              "Value": "OperStatus (1=UP,2=DOWN)"
            }
          }
        }
      ]
    },
    {
      "id": 2,
      "type": "table",
      "title": "Admin UP but Oper DOWN (possible issues) – selected switch",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "ifOperStatus{instance=\"$cumulus_switch\"} == 2 and on(instance,ifIndex,ifName,ifDescr,ifAlias) ifAdminStatus{instance=\"$cumulus_switch\"} == 1",
          "legendFormat": "{{instance}} {{ifName}}",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto", "displayMode": "auto" }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [ { "desc": false, "displayName": "ifName" } ]
      },
      "transformations": [
        { "id": "labelsToFields", "options": { "mode": "columns" } },
        {
          "id": "organize",
          "options": {
            "excludeByName": {},
            "indexByName": {
              "Time": 0,
              "ifName": 1,
              "ifDescr": 2,
              "ifAlias": 3,
              "Value": 4,
              "ifIndex": 5,
              "instance": 6,
              "job": 7,
              "__name__": 8
            },
            "renameByName": {
              "Value": "OperStatus (1=UP,2=DOWN)"
            }
          }
        }
      ]
    },
    {
      "id": 3,
      "type": "table",
      "title": "All DOWN interfaces (all switches)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "ifOperStatus == 2",
          "legendFormat": "{{instance}} {{ifName}}",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto", "displayMode": "auto" }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [ { "desc": false, "displayName": "instance" } ]
      },
      "transformations": [
        { "id": "labelsToFields", "options": { "mode": "columns" } },
        {
          "id": "organize",
          "options": {
            "excludeByName": {},
            "indexByName": {
              "Time": 0,
              "instance": 1,
              "ifName": 2,
              "ifDescr": 3,
              "ifAlias": 4,
              "Value": 5,
              "ifIndex": 6,
              "job": 7,
              "__name__": 8
            },
            "renameByName": {
              "Value": "OperStatus (1=UP,2=DOWN)"
            }
          }
        }
      ]
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Down ports per switch (OperStatus=2)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 16 },
      "targets": [
        {
          "refId": "A",
          "expr": "count by (instance) (ifOperStatus == 2)",
          "legendFormat": "{{instance}}",
          "range": true
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [ "lastNotNull" ],
          "fields": "",
          "values": false
        },
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          },
          "unit": "none"
        },
        "overrides": []
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Admin UP interfaces per switch",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 16 },
      "targets": [
        {
          "refId": "A",
          "expr": "count by (instance) (ifAdminStatus == 1)",
          "legendFormat": "{{instance}}",
          "range": true
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [ "lastNotNull" ],
          "fields": "",
          "values": false
